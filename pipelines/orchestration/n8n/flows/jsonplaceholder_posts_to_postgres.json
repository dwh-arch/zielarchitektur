      ]
    },
    {
      "parameters": {
        "functionCode": "return items.map(i => ({ json: {\n  source_name: 'jsonplaceholder.posts',\n  post_id: i.json.id,\n  user_id: i.json.userId,\n  title: i.json.title,\n  body: i.json.body\n}}));"
      },
      "id": "Map",
      "name": "Map Fields",
      "type": "n8n-nodes-base.function",
      "typeVersion": 2,
      "position": [
        760,
        220
      ]
    },
    {
      "parameters": {
        "operation": "executeQuery",
        "query": "insert into ods.api_posts(post_id, user_id, title, body, ingested_at)\nvalues ({{$json.post_id}}, {{$json.user_id}}, {{$json.title}}, {{$json.body}}, now())\non conflict (post_id) do update set user_id=excluded.user_id, title=excluded.title, body=excluded.body, ingested_at=excluded.ingested_at;"
      },
      "id": "PGInsert",
      "name": "Upsert Posts (Postgres)",
      "type": "n8n-nodes-base.postgres",
      "typeVersion": 2,
      "position": [
